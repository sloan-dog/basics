var num = 100;
function fizzbuzz(num) {
  var fac_five = false;
  var fac_three = false;
  var flag = false;
  var e = "";
  for( var i = 1; i < num + 1; i++) {
    fac_three = i % 3 === 0;
    fac_five = i % 5 === 0;
    flag = false;
    if (fac_three) {
      e += "fizz";
      flag = true;
    }
    if (fac_five) {
      e += "buzz";
      flag = true;
    }
    if (flag) {
      console.log(e);
    } else {
      console.log(i);
    }
    e = "";

  }
}
fizzbuzz(num)


