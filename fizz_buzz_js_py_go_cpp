/* 
Your previous Python 3 content is preserved below:

def fizzbuzz(num):
    if num < 1:
        return
    for i in range(1, num + 1):
        fac_five = i % 5 == 0
        fac_three = i % 3 == 0
        if fac_five and fac_three:
            print("fizzbuzz")
        elif fac_five:
            print("buzz")
        elif fac_three:
            print("fizz")
        else:
            print(i)
            

fizzbuzz(0)

 */